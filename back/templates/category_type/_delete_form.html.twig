<form method="post" action="{{ path('category_type_delete', {'idcategoryType': category_type.idcategoryType}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category_type.idcategoryType) }}">
    <button class="btn">Delete</button>
</form>
